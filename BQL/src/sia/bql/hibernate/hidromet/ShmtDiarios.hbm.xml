	<query name="consultaCaudalesEnEstacionYAno">
		<![CDATA[
			select n from ShmtDiarios as n 
			where n.drsEstacionId=:estacionId
			and n.drsAno=:año
			and n.drsVarId=:variable
			order by n.drsAno,n.drsMes
    	]]>
		<query-param name="estacionId" type="java.lang.Long" />
		<query-param name="año" type="java.lang.Long" />
		<query-param name="variable" type="java.lang.String" />
	</query>
